{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "index.js" },
    { "src": "/code/(.*)", "dest": "index.js" },
    { "src": "/((?!api/)[^/]+)", "dest": "index.js" }
  ]
}
